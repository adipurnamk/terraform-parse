# Terraform Parse Service Configuration
# Copy this file to .env and adjust values as needed

# AWS Configuration
TPS_AWS_REGION=eu-west-1

# Terraform Output Directory
# For Docker: use /app/generated
# For local: use ./generated
TPS_TERRAFORM_OUTPUT_DIR=./generated

# LocalStack Configuration (for Terraform testing)
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_DEFAULT_REGION=us-east-1
AWS_ENDPOINT_URL=http://localhost:4566

# Terraform Logging (optional)
# TF_LOG=DEBUG

# LocalStack Debug (optional)
# DEBUG=1

